    "import pandas as pd\n",
    "import numpy as np\n",
    "from sklearn.linear_model import LinearRegression as Lr\n",
    "data=pd.read_csv(\"headbrain.csv\")\n",
    "i=data.columns\n",
    "X=np.array(data[i[2]])\n",
    "Y=np.array(data[i[3]])\n",
    "X=X.reshape(-1,1)\n",
    "Y=Y.reshape(-1,1)\n",
    "model=Lr()\n",
    "model.fit(X,Y)\n",
    "model.predict(1530)"
   ]
  }
